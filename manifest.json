{
  "manifest_version": 2,
  "name": "Zillow Scraper",
  "description": "Scrape Zillow Data",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [ "tabs", "webNavigation","webRequest", "webRequestBlocking", "*://*/*" ],

  "browser_action": {
    "default_title": "Zillow Scraper"
  }
}


